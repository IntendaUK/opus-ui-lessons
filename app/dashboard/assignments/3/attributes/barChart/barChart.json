{
	"acceptPrps": {
		"dataIndex": {
			"type": "string"
		},
		"width": {
			"type": "string"
		}
	},
	"type": "containerSimple",
	"prps": {
		"backgroundColor": "#ce3fbb",
		"height": "35px",
		"margin": "4px",
		"borderRadius": "12px",
		"transform": "skewX(-8deg)",
		"width": "0%",
		"shadow": "2px 2px 5px #15181B",
		"transitionProperty": "width",
		"transitionDuration": "0.5s",
		"flows": [
			{
				"from": "||attrsContainer||",
				"fromKey": "barChartData",
				"fromSubKey": "%dataIndex%",
				"toKey": "width",
				"mapFunctionString": "v => v + '%'"
			}
		]
	}
}