					"relId": "input",
					"type": "input",
					"prps": {
						"placeholder": "This is a placeholder...",
						"flows": [
							{
								"to": "||container.label||",
								"toKey": "cpt",
								"ignoreEmptyString": true
							}
						]
					}
				},
